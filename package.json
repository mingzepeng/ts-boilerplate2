{
  "name": "ts-boilerplate2",
  "version": "1.0.0",
  "description": "a react boilerplate on typescript",
  "main": "./src/main.js",
  "scripts": {
    "start": "node ./server.js",
    "mock": "cd ./mock & json-server --watch db.json --routes routes.json --port 9001",
